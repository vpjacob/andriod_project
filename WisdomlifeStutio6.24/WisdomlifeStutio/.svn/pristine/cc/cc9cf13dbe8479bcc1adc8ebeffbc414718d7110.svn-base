/*@import "menu.css";*/
/** 清除内外边距 **/
body, h1, h2, h3, h4, h5, h6, hr, p,
blockquote, /* structural elements 结构元素 */
dl, dt, dd, ul, ol, li, /* list elements 列表元素 */
pre, /* text formatting elements 文本格式元素 */
form, fieldset, legend, button, input, textarea, /* form elements 表单元素 */
th, td, /* table elements 表格元素 */
img/* img elements 图片元素 */{
    border:medium none;
    margin: 0;
    padding: 0;
}
/** 设置默认字体 **/
body {
    margin: 0;
    padding: 0;
    font-size: 16px;
    font-family: '微软雅黑';
}
/*h1, h2, h3, h4, h5, h6 { font-size: 100%; }*/
/*em{font-style:normal;}*/
/** 重置列表元素 **/
ul, ol { list-style: none; }
/** 重置超链接元素 **/
a { text-decoration: none; color:#000;}
a:hover { text-decoration: underline; color:#3363a7; }
/** 重置图片元素 **/
img{ border:0px;}
/** 重置表格元素 **/
table { border-collapse: collapse; border-spacing: 0; }
.bar{
  background-color:#fff;
}
.typeAll{
    display: -webkit-box;
    display: -moz-box;
    display: -o-box;
    display: box;
    background-color: #fff;
}
.typeAll .same{
    -webkit-box-flex: 1;
    -moz-box-flex: 1;
    -o-box-flex: 1;
    box-flex: 1;
    text-align: center;
    font-size: 14px;
    color: #646464;
}
.line{
    height: 50px;
    width: 100%;
    position: relative;
}
.aLine{
    margin: 0 48px;
    height: 25px;
    border-bottom: 1px solid #bababa;
}
.lineR{
    width: 100px;
    background: #EFEFF4 url("/mobile/images/heart.png") no-repeat 4px center;
    text-align:center;
    position: absolute;
    left: 50%;
    top:10px;
    margin-left: -50px;
    font-weight: 600;
}

.aLi{
    border-bottom:1px solid #e7e7e7;
    padding:4px 10px;
    display: -webkit-box;
    display: -moz-box;
    display: -o-box;
    display: box;
    -webkit-box-flex: 1;
    -moz-box-flex: 1;
    -o-box-flex: 1;
    box-flex: 1;
}
.aLi img {
    margin-top: 5px;
}
.alist{
    width: 100%;
    background-color: #fff;
}
.conter{
    padding:0 10px;
    -webkit-box-flex: 1;
    -moz-box-flex: 1;
    -o-box-flex: 1;
    box-flex: 1;
}
.conterO,.price{
    display: -webkit-box;
    display: -moz-box;
    display: -o-box;
    display: box;
    -webkit-box-flex: 1;
    -moz-box-flex: 1;
    -o-box-flex: 1;
    box-flex: 1;
}
.conterOleft{
    display: -webkit-box;
    display: -moz-box;
    display: -o-box;
    display: box;
    -webkit-box-flex: 1;
    -moz-box-flex: 1;
    -o-box-flex: 1;
    box-flex: 1;
    font-size: 16px;
    font-family: '微软雅黑';
}
.conterOringht{
    display: -webkit-box;
    display: -moz-box;
    display: -o-box;
    display: box;
    -webkit-box-flex: 1;
    -moz-box-flex: 1;
    -o-box-flex: 1;
    box-flex: 1;
    -webkit-box-pack: end;
    -moz-box-pack: end;
    -o-box-pack: end;
    box-pack: end;
    font-size: 14px;
    color: #9b9b9b;
}
.describe{
    font-size: 12px;
    color: #9b9b9b;
}
.priceRight{
    color: #ff860f;
}
.priceLeft{
    color: #ff0000;
}
.priceDel{
    padding-left: 4px;
    font-size: 12px;
    color:#9b9b9b ;
}
.price{
    margin-top: 6px;
}
.icon-search:before {
    content:url(../../image/search.png);
}

.bar .icon {
    position:absolute;
    z-index: 20;
    font-size: 1rem;
    line-height: 1.2rem;
   padding: 0.25rem 0.0rem;
}