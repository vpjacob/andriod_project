	<script type='text/javascript' src='../../script/zepto.min.js' charset='utf-8'></script>
	<script type="text/javascript" src="../../script/common.js"></script>
	<script type="text/javascript" src="../../script/guanjia/guanjia.js"></script>
	<script type="text/javascript">
		$(".tankuang_box,.black_box").hide()
		$("#wuye").click(
			function(){
				$(".tankuang_box,.black_box").show()
			}
		)
		$("#closetankuang").click(
			function(){
				$(".tankuang_box,.black_box").hide()
			}
