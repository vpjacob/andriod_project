.bar{
  background-color:#fff;
}
.content{
   background-color:#f0f0f0;
}

.list-block{
  background-color:#fff;
  margin:0px;
}

.content>span{
   padding:3px 10px;
   font-size:.85rem;
}

.list-block .item-title{
   width:40%;
   font-size:16px;
}
.item-input input{
  text-align:right;
}
.list-block select{
  direction: rtl;
  font-size:16px;
  width:93%;
}

.list-block select option{
  direction: ltr;
}
