.item-content{
  border-bottom:1px solid #f0f0f0;
}

.dizhi{
  float:right;
  margin-right:10px;
}

::-webkit-clear-buttonm,::-webkit-calendar-picker-indicator {
     -webkit-appearance: none; 
}

::-webkit-inner-spin-button { visibility: hidden; }

.provices,.cities{
  width: 45%;
  
  display:inline-block;  
}

.cities{
  float:right;
}
.provices select,.cities select{
  padding-right:10px;
}
