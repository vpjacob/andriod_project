/*@import "menu.css";*/
/** 清除内外边距 **/
body, h1, h2, h3, h4, h5, h6, hr, p,
blockquote, /* structural elements 结构元素 */
dl, dt, dd, ul, ol, li, /* list elements 列表元素 */
pre, /* text formatting elements 文本格式元素 */
form, fieldset, legend, button, input, textarea, /* form elements 表单元素 */
th, td, /* table elements 表格元素 */
img/* img elements 图片元素 */{
    border:medium none;
    margin: 0;
    padding: 0;
}
/** 设置默认字体 **/
body {
    margin: 0;
    padding: 0;
    font-size: 16px;
    font-family: '微软雅黑';
}
/*h1, h2, h3, h4, h5, h6 { font-size: 100%; }*/
/*em{font-style:normal;}*/
/** 重置列表元素 **/
ul, ol { list-style: none; }
/** 重置超链接元素 **/

a:hover { text-decoration: underline; color:#3363a7; }
a ,a:hover{ text-decoration: none; color:#000;}
/** 重置图片元素 **/
img{ border:0px;}
/** 重置表格元素 **/
table { border-collapse: collapse; border-spacing: 0; }
.page,.page-group{
   background-color:#f0f0f0;
}
.bar {
   background-color:#ffffff;
}

.line {
	height: 50px;
	width: 100%;
	position: relative;
}

.icon-search:before {
	content: url(../../image/search.png);
}

.bar-header-secondary {
	height: 44px;
	padding-right: .5rem;
	padding-left: .5rem;
	backface-visibility: hidden;
	width: 70%;
	margin: 0px auto;
}

.choose{
	margin-top: 15px;
}
#search {
	background-color: #fff;
	border: 1px solid #cccccc;
	height: 1.6 rem;
	border-radius: 20px;
}

.bar .icon-dizhi {
	font-size: 20px;
	color: #0eaae3;
	padding: 0;
	top:0px;
	right:30px;
}
.ditu{
   position:absolute;
   bottom:3px;
   right:30px;
   font-size:12px;
}
.bar .icon {
	position: absolute;
	z-index: 20;
	font-size: 1rem;
	line-height: 1.2 rem;
	padding: 0.25 rem 0.0 rem;
}

.list-block{
  margin-top:10px;
}
.item-subtitle{
  color:#787878;
}
.icon-favorfill{
  color:#C32C47;
}
.item-text span{
   color:#fff;
   background-color:#0eaae0;
   font-size:14px;
   border-radius:3px;
   padding:0px 3px;
   display:inline;
}
.list-block .item-text{
   height:auto;
}
.list-block .item-subtitle{
   margin:5px 0px;
}

.list-block li{
  border-bottom:1px solid #f0f0f0;
}
