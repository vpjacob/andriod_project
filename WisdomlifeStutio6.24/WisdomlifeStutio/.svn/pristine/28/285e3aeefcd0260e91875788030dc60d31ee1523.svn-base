/*@import "menu.css";*/
/** 清除内外边距 **/
body, h1, h2, h3, h4, h5, h6, hr, p, blockquote, /* structural elements 结构元素 */
dl, dt, dd, ul, ol, li, /* list elements 列表元素 */
pre, /* text formatting elements 文本格式元素 */
form, fieldset, legend, button, input, textarea, /* form elements 表单元素 */
th, td, /* table elements 表格元素 */
img/* img elements 图片元素 */ {
	border: medium none;
	margin: 0;
	padding: 0;
}
/** 设置默认字体 **/
body {
	margin: 0;
	padding: 0;
	font-size: 16px;
	font-family: '微软雅黑';
}
/*h1, h2, h3, h4, h5, h6 { font-size: 100%; }*/
/*em{font-style:normal;}*/
/** 重置列表元素 **/
ul, ol {
	list-style: none;
}
/** 重置超链接元素 **/
a {
	text-decoration: none;
	color: #000;
}
a:hover {
	text-decoration: underline;
	color: #3363a7;
}
/** 重置图片元素 **/
img {
	border: 0px;
}
/** 重置表格元素 **/
table {
	border-collapse: collapse;
	border-spacing: 0;
}

#login_do,#login_do2{
  margin-right:1.3rem;
}
.step {
	width: 100%;
	height: 40px;
	line-height: 40px;
	background: #fff url("../../mobile/images/jiantou.png") no-repeat center;
}
.step span {
	display: inline-block;
	width: 49%;
	text-align: center;
	font-size: 14px;
}
.focusBlue {
	color: #0eaae3;
}
.paddingOne, .paddingTwo {
	padding: 0 10px;
	background: #fff;
}
.codes {
	border-radius: 25px;
	padding: 0 10px;
	background-color: #fff;
	color: #0eaae3;
	height: 30px;
	width: 100%;
	font-size: 12px;
	border: 1px solid #0eaae3;
}
.doCodes {
	border-radius: 25px;
	padding: 0 10px;
	background-color: #fff;
	color: #bbbbbb;
	height: 30px;
	width: 100%;
	font-size: 12px;
	border: 1px solid #bbbbbb;
}
.focusGray {
	color: #a0a0a0;
}

.tips {
	width: 180px;
	height: 130px;
	background-color: #3b3c3c;
	position: absolute;
	top: 30%;
	left: 50%;
	margin-left: -90px;
	border-radius: 5px;
	text-align: center;
	display: none;
}
.tips img {
	margin-top: 16px;
}
.tips p {
	color: #fff;
	font-size: 18px;
}
.aline {
	color: #a7a7a7;
	font-size: 12px;
	padding: 0 10px;
}
.item-title {
	font-size: 15px;
	font-weight: 600;
}
.list-block .item-inner .again {
	display: none;
}
.lengthtips {
	padding: 0 10px;
	font-size: 14px;
	color: #7b7b7c;
}
#vaildCodes {
	text-align: right;
}
.bgpic {
	width: 100%;
	height: 230px;
	background: url("../../mobile/images/device_info_logo.9.png") no-repeat center;
	background-size: cover;
	margin-bottom: 10px;
}
.content{
	margin-top:20px;
}

