/*清除浮动*/
.clear {
	height: 0px;
	line-height: 0px;
	font-size: 0px;
	clear: both;
}

.know{
   width:100%;
   background-color:#fff;
}
.know>div{
   text-align:center;
   width:100%;
   margin-top:10px;
   line-height: 30px;
   color:#787878;
}

ul{
  width:100%;
  font-size:14px;
  padding-left:10px;
  margin-top:5px;
  color:#787878;
}

.jia{
	
   text-align:center;
   background-color:#fff;
   margin-top:10px;
   height:70px
}
.jia .left,.jia .middle{
	float:left;
	text-align:left;
	font-size:15px;
	
}
.jia .middle span{
	margin-top: 10px;
}
.jia .last{
	float:right;
	margin: 23px 20px 0 0;
}
.jia .right{
	float:right;
	width:35px;
	margin-top:25px;
	margin-right:10px;
}
.jia img{width:40px;height:40px;margin:15px 15px;}
