.icon-roundclose{
   color:#0eaae0;  
}

.bank{
  background-color:#fff;
  margin-top:10px;
}

.item-content{
  padding:10px;
}

.item-content>div{
   display: inline-block;
}

.item-input{
   float:right;
   margin-right:10px;
}

.item-input input{
  text-align:right;
}

select{
  /*-webkit-appearance:none;*/
  border:none;
}
.item-input{
  position:relative;
}
.item-input ul{
  position:absolute;
  background-color:#444444;
  color:#fff;
  width:120%;
  z-index:99;
  padding-left:0;
  font-size:16px;
  display:none;
}

.item-input ul li{
   border-bottom: 1px solid #BBBBBB;
   padding:8px 8px;
} 

.secondul{
   right:0;
}

.last{
  border:none;
}

.item-input .first{
   width:5px;
   height:5px;
   position:absolute;
   z-index:98;
   -webkit-transform:rotate(45deg);
   transform:rotate(45deg);
   top:-7px;
   right:15px;
   background-color:#444444;
   border:none;
   
}

.icon-down{
   font-size:20px;
}
.commit{
   width:90%;
   margin:20px auto;
   background-color:#0EAAE0;
   color:#FFFFFF;
   padding:8px 0;
   text-align:center;
   border-radius:10px;
}
#selectBank {
	width:150px;
	border:1px solid #E5E5E5;
	border-radius:5px;
	float:right;
	color:#A9A9A9;
	background-color: white;
}
