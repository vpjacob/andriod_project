/*@import "menu.css";*/
/** 清除内外边距 **/
body, h1, h2, h3, h4, h5, h6, hr, p,
blockquote, /* structural elements 结构元素 */
dl, dt, dd, ul, ol, li, /* list elements 列表元素 */
pre, /* text formatting elements 文本格式元素 */
form, fieldset, legend, button, input, textarea, /* form elements 表单元素 */
th, td, /* table elements 表格元素 */
img/* img elements 图片元素 */{
    border:medium none;
    margin: 0;
    padding: 0;
}
/** 设置默认字体 **/
body {
    margin: 0;
    padding: 0;
    font-size: 16px;
    font-family: '微软雅黑';
}
/*h1, h2, h3, h4, h5, h6 { font-size: 100%; }*/
/*em{font-style:normal;}*/
/** 重置列表元素 **/
ul, ol { list-style: none; }
/** 重置超链接元素 **/
a { text-decoration: none; color:#000;}
a:hover { text-decoration: underline; color:#3363a7; }
/** 重置图片元素 **/
img{ border:0px;}
/** 重置表格元素 **/
table { border-collapse: collapse; border-spacing: 0; }

/*清除浮动*/
.clear{
    height:0px;
    line-height:0px;
    font-size:0px;
    clear:both;
}
#list-block, #list-blocks {
    margin: 0.75rem 0;
    font-size: .85rem;
}
.label{
    font-size: 14px;
}
#list-block select, input, .item-inner,.fapplicant, .ftelphone, .fcode{
    font-size: 14px;
}
#list-block ul,#list-blocks ul {
    background-color:#EFEFF4;
}
#list-block ul li ,#list-blocks ul li {
    background-color: #fff;
}
.item-content{

}
.list-block .item-input{
    text-align: right;
}
.buttons {
    background-color: #1fb8f0;
   color: #fff;
    text-decoration: none;
    text-align: center;
    display: block;
    box-sizing: border-box;
    -webkit-appearance: none;
    -moz-appearance: none;
    -ms-appearance: none;
    appearance: none;
    padding: 0 .5rem;
    margin: 0;
    white-space: nowrap;
    position: relative;
    text-overflow: ellipsis;
    font-family: inherit;
    cursor: pointer;
    height: 40px;
    line-height: 40px;
    font-size: 16px;
    border-radius: 25px;
}
.item-input{
    text-align: right;
}
#smansionid{
    font-size: 14px;
    text-align: right;
}
.list-block .item-media+.item-inner{
    border-bottom: 1px solid #d8d8d8;;
}
.changes{
    font-size: 14px;
    text-align: right;
}
.item-input input{
    text-align: right;
}
.warning{
    color: #a6a6a6;
    font-size: 12px;
    padding: 0 20px;
}
.warning span{
    background: url("/mobile/images/tips.png") no-repeat left center;
    padding-left: 20px;
    color: #000;
}
.marginTop{
    margin-top: 5px;
}
.itemtop{
    border-top:1px solid #d8d8d8;
}
.itembottom{
    border-bottom:1px solid #d8d8d8;
}

select {
    direction: rtl;
}
select option {
    direction: ltr;
}
.item-contentv{
    padding-left:0px;
}
.item-innerv{
    margin-left: 0px;
    padding: 0 14px;
}

.page.page-curren,.content{
   background-color:#f0f0f0;
}