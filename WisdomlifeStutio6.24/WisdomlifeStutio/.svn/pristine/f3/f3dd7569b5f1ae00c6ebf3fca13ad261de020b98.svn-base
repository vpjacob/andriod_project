/*清除浮动*/
.clear {
	height: 0px;
	line-height: 0px;
	font-size: 0px;
	clear: both;
}

.imgtop{
    text-align:center;
    width:100%;
    height:250px;
    background: url("../../image/loveback.jpg") no-repeat center;
    background-size:cover;
    background-color:rgba(0,0,0,.9); 
}

.imgbottom,.imgbottom img{
    width:100%;
}

.content-block{
   margin:0;
   padding:0;
}

.tabs{
   text-align:center;
}



.imgtop>div{
  width:100%;
  height:100%;
  background-color:rgba(0,0,0,.4); 
