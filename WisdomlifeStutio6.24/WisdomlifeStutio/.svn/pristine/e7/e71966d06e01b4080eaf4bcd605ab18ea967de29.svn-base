/*@import "menu.css";*/
/** 清除内外边距 **/
body, h1, h2, h3, h4, h5, h6, hr, p,
blockquote, /* structural elements 结构元素 */
dl, dt, dd, ul, ol, li, /* list elements 列表元素 */
pre, /* text formatting elements 文本格式元素 */
form, fieldset, legend, button, input, textarea, /* form elements 表单元素 */
th, td, /* table elements 表格元素 */
img/* img elements 图片元素 */{
    border:medium none;
    margin: 0;
    padding: 0;
}
/** 设置默认字体 **/
body {
    margin: 0;
    padding: 0;
    font-size: 16px;
    font-family: '微软雅黑';
}
/*h1, h2, h3, h4, h5, h6 { font-size: 100%; }*/
/*em{font-style:normal;}*/
/** 重置列表元素 **/
ul, ol { list-style: none; }
/** 重置超链接元素 **/
a { text-decoration: none; color:#000;}
a:hover { text-decoration: underline; color:#3363a7; }
/** 重置图片元素 **/
img{ border:0px;}
/** 重置表格元素 **/
table { border-collapse: collapse; border-spacing: 0; }
.clearfix{
	clear:both;
}
.top{
	width:100%;
	height:110px;
	background-color: #0EAAE3;
	
}
.top .topFirst{
	width:100%;
	height:110px;
	border-bottom:1px solid #5CC5F2;
}
.top .topFirst span{
	display:block;
	text-align: center;
	color: white;
}
.top .topFirst span:nth-child(1){
	line-height: 95px;
	font-size: 26px;
}
.top .topFirst span:nth-child(2){
	margin-top: -29px;
}
/* .top .topSec{
	margin-top:15px;
	width:100%;
	height:60px;
} */
.top .topSec span{
	color: white;
}
.top .topSec .left{
	width:50%;
	float: left;
	height: 60px;
	border-right:1px solid #5CC5F2;
}
.top .topSec .right{
	width:49%;
	float: right;
	height: 60px;
}
.top .topSec .left span:nth-child(1){
	line-height: 40px;
	text-align: center;
	display: block;
}
.top .topSec .left span:nth-child(2){
	text-align: center;
	display: block;
	margin-top: -5px;
	font-size: 14px;
}
.top .topSec .right span:nth-child(1){
	line-height: 40px;
	text-align: center;
	display: block;
}
.top .topSec .right span:nth-child(2){
	text-align: center;
	display: block;
	margin-top: -5px;
	font-size: 14px;
}
.bottom{
	width: 100%;
}
.bottom .same{
	width: 100%;
	border-bottom:1px solid #CCCCCC;
	height: 70px;
	margin-top:5px;
}
.bottom .same .left{
	width:33%;
	float:left;
	height: 100%;
	line-height: 70px;
	padding-left:5px;
}
.bottom .same .mid{
	width:33%;
	float:left;
	height: 100%;
}
.bottom .same .mid img{
	margin-top:16px;
	width:36px;
	height: 29px;
}
.bottom .same .mid span{
	
	position:relative;
	top:-5px;
	left: 5px;
}
.bottom .same .right{
	width:33%;
	float:left;
	height: 100%;
}
.bottom .same .right img{
	margin-top:14px;
	
}
.bottom .same .right span{
	
	position:relative;
	top:-10px;
	left: 5px;
}
.bottom .date{
	width:100%;
	height: 30px;
	border-bottom:1px solid #CCCCCC;
	padding-left:5px;
}
