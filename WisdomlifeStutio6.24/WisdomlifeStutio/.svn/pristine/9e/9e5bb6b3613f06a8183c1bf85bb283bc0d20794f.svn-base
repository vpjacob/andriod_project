/*@import "menu.css";*/
/** 清除内外边距 **/
body, h1, h2, h3, h4, h5, h6, hr, p,
blockquote, /* structural elements 结构元素 */
dl, dt, dd, ul, ol, li, /* list elements 列表元素 */
pre, /* text formatting elements 文本格式元素 */
form, fieldset, legend, button, input, textarea, /* form elements 表单元素 */
th, td, /* table elements 表格元素 */
img/* img elements 图片元素 */{
    border:medium none;
    margin: 0;
    padding: 0;
}
/** 设置默认字体 **/
body {
    margin: 0;
    padding: 0;
    font-size: 16px;
    font-family: '微软雅黑';
}
/*h1, h2, h3, h4, h5, h6 { font-size: 100%; }*/
/*em{font-style:normal;}*/
/** 重置列表元素 **/
ul, ol { list-style: none; }
/** 重置超链接元素 **/
a { text-decoration: none; color:#000;}
a:hover { text-decoration: underline; color:#3363a7; }
/** 重置图片元素 **/
img{ border:0px;}
/** 重置表格元素 **/
table { border-collapse: collapse; border-spacing: 0; }
.aui-nav ul li{
	float:left;
	width:33.33%;
}
.aui-nav,
.aui-bar-tab{
	width:100%;
	height:48px;
	position:absolute;
	bottom:0;
	border-top: 1px solid #dcdcdc;
	background-color:#FFFFFF;
}
.aui-nav 
p{
	font-size:12px;
	color:#787878;
	text-align:center;
}
.iconfont1{
	width: 26px;
    position: relative;
    left: 50%;
    margin-left: -13px;
}
.content,
ul{
	
	background-color: #FFFFFF;
}
.content
li{
	width:100%;
	height:60px;
}
.downloadlist1{
	width:80px;
	height:50px;
	margin-left:0.5rem;
	border-radius:3px;
	float:left;
	position:relative;
	margin-top:5px;
}
.downloadlist1
img{
	width:80px;
	height:50px;
	border-radius:3px;
	position:absolute;
}
.downloadlist_right{
	width:70%;
	height:60px;
	border-bottom:1px solid #DCDCDC;
	position:relative;
	float:right;
}
.downloadlist_right
p{
	width:80%;
	line-height:40px;
	display:block;
	white-space:nowrap;
	text-overflow:ellipsis;
	overflow: hidden;
}

.content{
  height:100%;
  overflow:scroll;
  margin-top: 10px;
}



















