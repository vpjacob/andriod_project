#personnel-dynamic .facebook-name {
	position: relative;
	width: 240px;
	margin-bottom: 10px;
	margin-left: 10px;
}

#personnel-dynamic .card {
	height: 100%;
	margin-left: 0px;
	margin-right: 0px;
	margin-top: 0px;
	box-shadow:none;
}

#personnel-dynamic .card-header {
	margin-top: 0px;
	height: 20%;
	padding-bottom: 0px;
	padding-left: 2px;
	padding-right: 5px;
}

#personnel-dynamic .card-header-frist {
	margin-top: 50px;
}

#personnel-dynamic .card-header:after {
	width: 83%;
	left: 17%;
}

#personnel-dynamic .card-content {
	height: 70%;
}

#personnel-dynamic .card-footer {
	height: 10%;
	padding-top: 0px;
}

#personnel-dynamic .card-content-inner {
	margin-left: 13%;
	padding-top: 5px;
	padding-bottom: 0px;
	padding-left: 15px;
	padding-right: 15px;
}

#personnel-dynamic .card-content-inner img {
	/*width: 30%;
	 height: 90%;*/
}

#personnel-dynamic .card-content-inner p {
	margin-bottom: 0px;
	color: #7C7C7C;
	font-size: 14px;
	font-weight: 400;
}

#personnel-dynamic .card-footer span {
	font-size: 12px;
	color: #C4C4C4;
}

#personnel-dynamic .card-footer:before {
	height: 0px;
}

#personnel-dynamic .card-footer-title {
	margin-left: 14%;
}

#personnel-dynamic .icon-angle-down {
	color: #BBBBBB;
	font-size: 14px;
}
#personnel-dynamic div[class*="key-col-"] {
	background: #0EAAE3;
	text-align: center;
	color: white;
	padding: 5px;
	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
	border-radius: 3px;
}


