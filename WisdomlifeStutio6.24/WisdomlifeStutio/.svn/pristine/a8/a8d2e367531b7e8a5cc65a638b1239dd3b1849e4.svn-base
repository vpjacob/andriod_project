#personnel-dynamic .tab-a {
	border: 0;
}
/*女*/
#personnel-dynamic .sex-bgc2 {
	background-color: #FA94AA;
	border-radius: 2px;
}

/*男*/
#personnel-dynamic .sex-bgc1 {
	background-color: #4AAEF3;
	border-radius: 2px;
}

#personnel-dynamic .sex {
	width: 24px;
	height: 12px;
	margin-left: 4px;
	margin-top: 4px;
}

#personnel-dynamic .sex span {
	float: left;
	width: 50%;
	height: 10px;
	color: white;
	text-align: center;
	font-size: 10px;
	position: relative;
}

#personnel-dynamic .img1 {
	float: left;
	width: 30%;
	height: ;
	padding-top: 2px;
	padding-bottom: 2px;
	margin-right: 2px;
	margin-left: 2px;
}

#personnel-dynamic .vip {
	width: 36px;
	float: left;
	height: 12px;
	margin-left: 2px;
	margin-top: 2px;
}

#personnel-dynamic .img2 {
	width: 100%;
	padding-left: 2px;
	height: 100%;
	margin-bottom: 5px;
	margin-top: 2px;
	z-index: 1;
}

#personnel-dynamic .list-block.media-list .item-link .item-title-row, #personnel-dynamic .list-block li.media-item .item-link .item-title-row {
	background-image: url(../../image/nearby/ic_common_arrow_bottom.png);
	background-size: 15px 8px;
}

#personnel-dynamic .title {
	width: 100%;
	float: left;
}

#personnel-dynamic .title-name {
	width: auto;
	float: left;
	height: 100%;
	margin-left: 2px;
	position: relative;
}

#personnel-dynamic .titlespan {
	font-size: 16px;
	font-weight: 300;
	display: block;
}

#personnel-dynamic .facebook-dateafter {
	font-size: 12px;
	margin-bottom: 30px;
	color: #BBBBBB;
	width: 100px;
	text-align: right;
}

#personnel-dynamic .facebook-avatar img {
	width: 100%;
	height: 100%;
	border-radius: 5px;
}

#personnel-dynamic .sectitle {
	width: 100%;
	margin-top: 1px;
}

#personnel-dynamic .sectitle span {
	width: 100%;
	font-size: 12px;
	color: #AEAEAE;
	display: block;
	margin-left: 2px;
}

