.clock{
    width: 100%;
    padding: 10px;
    background: rgba(41,97,156,0.8);
}
.clock{
    display: -webkit-box;
    display: -moz-box;
    display: -o-box;
    display: box;
    -webkit-box-flex: 1;
    -moz-box-flex: 1;
    -o-box-flex: 1;
    box-flex: 1;
    color: #fff;
}
.clock_left{
    -webkit-box-flex: 1;
    -moz-box-flex: 1;
    -o-box-flex: 1;
    box-flex: 1;
    color: #fff;
}
.colck_right{
    -webkit-box-pack: end;
    -moz-box-pack: end;
    -o-box-pack: end;
    box-pack: end;
}
.change_right{
    -webkit-box-pack: end;
    -moz-box-pack: end;
