}
.exponent{
    width: 100%;
    background: rgba(41,97,156,0.8);
    color: #fff;
    margin-top: 10px;
    padding-bottom: 20px;
}
.exponent p{
    width: 100%;
    padding:10px;
    border-bottom: 1px solid #8ca9c9;
}
.exponent_same{
    display: -webkit-box;
    display: -moz-box;
    display: -o-box;
    display: box;
    width: 100%;
    border-bottom: 1px solid #8ca9c9;
}
.exponent_left{
    width: 50%;
    display: -webkit-box;
    display: -moz-box;
    display: -o-box;
    display: box;
    -webkit-box-align: center;
    -moz-box-align: center;
    -o-box-align: center;
    box-align: center;
    padding: 0px 16px;
}
.exponent_left .exponent_lc{
    -webkit-box-flex: 1;
    -moz-box-flex: 1;
    -o-box-flex: 1;
    box-flex: 1;
    font-size: 34px;
    -webkit-box-pack: center;
    -moz-box-pack: center;
    -o-box-pack: center;
    box-pack: center;
}
.exponent_left .exponent_rc{
    -webkit-box-pack: end;
    -moz-box-pack: end;
    -o-box-pack: end;
    box-pack: end;
    text-align: right;
}
.exponent_left .exponent_rc span{
    display: block;
}
.exponent_rc .exponent_rcolor{
    color: #a8bace;
}
.exponent_rc .exponent_rsize{
    font-size: 18px;
}
.exponent_lc img{
    padding-top: 16px;
}
