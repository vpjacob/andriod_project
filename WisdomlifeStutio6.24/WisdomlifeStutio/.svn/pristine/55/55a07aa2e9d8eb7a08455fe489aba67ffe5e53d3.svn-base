body {
	font-family: "微软雅黑";
	background-color: #fff;
}
img {
	display: block;
}
input {
	margin: 0;
	padding: 0;
	outline: none;
	border: none;
	text-align: right;
}
.clearfix {
	content:'';
	clear:both;
	display:block;
	width: 0;
	height: 0;
	visibility:hidden;
	overflow: auto;	
}
::-webkit-input-placeholder { /* WebKit browsers */
	color: #CCCCCC;;
}
:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
	color: #CCCCCC;
}
::-moz-placeholder { /* Mozilla Firefox 19+ */
	color: #CCCCCC;
}
:-ms-input-placeholder { /* Internet Explorer 10+ */
	color: #CCCCCC;
}


.title{
	height: 44px;
	background: #F08200;
}
.title .title_div{
	background-color: white;
	width:60%;
	height: 30px;
	border-radius: 20px;
	position: relative;
	top:7px;
	left:20%;
}
.title .title_div img{
	width:18px;
	height:18px;
	display:inline;
	position: absolute;
	top:6px;
	left:10px;
}
.title .title_div input{
	width:110px;
	display:inline;
	position: absolute;
	top:5px;
	left:35px;
	font-size:15px;
}

.top{
	position: relative;
	top:44px;
}
.top img{
	width:100%;
	height: 130px;
}
.deal{
	position: relative;
	top:44px;
}
.deal .deal_top{
	height:56px;
	background-color:#282E44;
}
.deal .deal_top .left{
	color:#5789F4;
	width:50%;
	float:left;
	text-align: center;
	border-right:1px solid #484E64;
	height: 100%;
}
.deal_mid{
	margin-top: 5px;
	margin-left:5px;
}
.deal .deal_top .left span:nth-child(1){
	width:100%; 
}
.deal .deal_top .left span:nth-child(2){
	color:white;
	font-size:14px;
}
.deal .deal_top .right{
	color:#5789F4;
	width:50%;
	float:right;
	text-align: center;
	height: 100%;
}

.deal .deal_top .right span:nth-child(1){
	width:100%; 
}

.deal .deal_top .right span:nth-child(2){
	color:white;
	font-size:14px;
}
.deal .deal_mid .lunbo{
	height: 38px;
	border-bottom:1px solid #A3A3A3
}
.deal_bottom{
	margin-top:2px;
}
.deal_bottom .detial{
	height: 95px;
	border-bottom:1px solid #D7D7D7;
}
.deal_bottom .detial .box{
	width:95%;
	height: 90px;
	margin-top: 5px;
	margin-left: 4px;	
}
.deal_bottom .detial .box .left{
	width:85px;
	height:85px;
	float:left;
}
.deal_bottom .detial .box img{
	width:85px;
	height:85px;
}
.deal_bottom .detial .box .right{
	float:left;
	height:85px;
	margin-left: 10px;
	
}
.deal_bottom .detial .box i{
	margin-top: 29px;
}
#sjaddress{
	width:100%;
	margin-top: 2px;
	font-size: 14px;
	color: #949494;
}
.icon-favorfill {
    color: #cd2647;
    float: left;
    margin-top: 5px;
}
.deal_bottom .detial .other{
	font-size: 14px;
	color: white;
	background-color: #0EAAE3;
	float: left;
	margin-top: 6px;
	border-radius: 5px;
	margin-left: 5px;
}
.same{
	width:80px;
	
}
